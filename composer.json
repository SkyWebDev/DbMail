{
  "name": "skywebdev/laravel-db-mail",
  "description": "Allow users to edit email (blade) templates",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Goran Savkic",
      "email": "savgoran@gmail.com"
    }
  ],
  "keywords": [
    "skywebdev",
    "db-mail",
    "blade",
    "laravel",
    "laravel-package"
  ],
  "autoload": {
    "psr-4": {
      "SkyWebDev\\DbMail\\": "src",
      "SkyWebDev\\Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SkyWebDev\\DbMail\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0",
    "laravel/framework": "^9.28|^10.0|^11.0|^12.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "SkyWebDev\\DbMail\\DbMailProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true
  }
}
